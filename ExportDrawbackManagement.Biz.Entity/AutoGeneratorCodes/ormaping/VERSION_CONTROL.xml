<?xml version="1.0"?>
<ORMapping tableName="VERSION_CONTROL" className="PLM.Biz.Entity.T_VersionControl" displayName="整合数据版本控制" assemblyName="PLM.Biz.Entity">
  <KeyItem>
    <Property>
      <Name>Area</Name>
      <DisplayName>地区范围</DisplayName>
      <Comment>地区范围：N-全国；L-本地。</Comment>
    </Property>
    <Column>
      <Name>AREA</Name>
      <Type>AnsiString</Type>
      <MaxLength>1</MaxLength>
      <CanBeNull>False</CanBeNull>
    </Column>
    <Transfer>
      <PropertyToColumn>True</PropertyToColumn>
      <ColumnToProperty>True</ColumnToProperty>
      <IsLogModify>True</IsLogModify>
    </Transfer>
  </KeyItem>
  <KeyItem>
    <Property>
      <Name>PeriodLen</Name>
      <DisplayName>周期长度</DisplayName>
      <Comment>周期长度：M-月度；Q-季度；Y-年度。</Comment>
    </Property>
    <Column>
      <Name>PERIOD_LEN</Name>
      <Type>AnsiString</Type>
      <MaxLength>1</MaxLength>
      <CanBeNull>False</CanBeNull>
    </Column>
    <Transfer>
      <PropertyToColumn>True</PropertyToColumn>
      <ColumnToProperty>True</ColumnToProperty>
      <IsLogModify>True</IsLogModify>
    </Transfer>
  </KeyItem>
  <KeyItem>
    <Property>
      <Name>PeriodId</Name>
      <DisplayName>周期标识（PIC随PERIOD_LEN</DisplayName>
      <Comment>周期标识（PIC随PERIOD_LEN：M-YYYYMM；Q-YYYYQ；Y-YYYY。）</Comment>
    </Property>
    <Column>
      <Name>PERIOD_ID</Name>
      <Type>AnsiString</Type>
      <MaxLength>6</MaxLength>
      <CanBeNull>False</CanBeNull>
    </Column>
    <Transfer>
      <PropertyToColumn>True</PropertyToColumn>
      <ColumnToProperty>True</ColumnToProperty>
      <IsLogModify>True</IsLogModify>
    </Transfer>
  </KeyItem>
  <KeyItem>
    <Property>
      <Name>TrafMode</Name>
      <DisplayName>运输方式</DisplayName>
      <Comment>运输方式：全国：空格-不分运输方式；5-空运。 本地：空格。</Comment>
    </Property>
    <Column>
      <Name>TRAF_MODE</Name>
      <Type>AnsiString</Type>
      <MaxLength>1</MaxLength>
      <CanBeNull>False</CanBeNull>
    </Column>
    <Transfer>
      <PropertyToColumn>True</PropertyToColumn>
      <ColumnToProperty>True</ColumnToProperty>
      <IsLogModify>True</IsLogModify>
    </Transfer>
  </KeyItem>
  <MemberItem>
    <Property>
      <Name>Version</Name>
      <DisplayName>版本</DisplayName>
      <Comment>版本：0-周期未满；非0的1位数字-周期满后的月数。</Comment>
    </Property>
    <Column>
      <Name>VERSION</Name>
      <Type>Int32</Type>
      <MaxLength>-1</MaxLength>
      <CanBeNull>True</CanBeNull>
    </Column>
    <Transfer>
      <PropertyToColumn>True</PropertyToColumn>
      <ColumnToProperty>True</ColumnToProperty>
      <IsLogModify>True</IsLogModify>
    </Transfer>
  </MemberItem>
  <MemberItem>
    <Property>
      <Name>UpdateDate</Name>
      <DisplayName>更新时间</DisplayName>
      <Comment>更新时间</Comment>
    </Property>
    <Column>
      <Name>UPDATE_DATE</Name>
      <Type>DateTime</Type>
      <MaxLength>-1</MaxLength>
      <CanBeNull>True</CanBeNull>
    </Column>
    <Transfer>
      <PropertyToColumn>True</PropertyToColumn>
      <ColumnToProperty>True</ColumnToProperty>
      <IsLogModify>True</IsLogModify>
    </Transfer>
  </MemberItem>
</ORMapping>